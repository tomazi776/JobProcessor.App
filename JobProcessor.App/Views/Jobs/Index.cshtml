@model JobProcessor.App.ViewModels.JobsVM
@using JobProcessor.App.ViewModels;

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<script src="~/Scripts/jquery-3.4.1.js"></script>
<script src="@Url.Content("~/Scripts/jquery.unobtrusive-ajax.js")" type="text/javascript"></script>

<div class="container">
    <row class="row ml-2 mt-5 ">
        <div class="col-6">
            <h1 id="title" class="">List of Jobs</h1>
        </div>
        <div class="col-3"></div>
        <div class="col-3">
            <nav class="mb-5 ml-5" aria-label="Page navigation">
                <div>
                    @Html.ActionLink("Create New", "Create", null, new { @class = "btn btn-outline-dark btn-lg " })
                </div>
            </nav>
        </div>
    </row>
</div>

<div id="jobsGrid">
    @Html.Partial("GetAll", Model)
</div>

<script src="~/Scripts/jquery-3.4.1.js"></script>



